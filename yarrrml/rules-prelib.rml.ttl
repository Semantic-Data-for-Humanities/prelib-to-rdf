:rules_000 void:exampleResource :map_oeuvres-langues_000.
:map_oeuvres-langues_000 rml:logicalSource :source_004.
:source_004 a rml:LogicalSource;
    rml:source "https://crbc-dataset.huma-num.fr/prelib.json?sql=select+code_iso_639_3%2C+oeuvre_id+from+prelib_oeuvre_langue+JOIN+prelib_langue+ON+prelib_langue.id+%3D+prelib_oeuvre_langue.langue_id&_shape=objects";
    rml:iterator "$.rows[*]";
    rml:referenceFormulation ql:JSONPath.
:map_oeuvres-langues_000 a rr:TriplesMap;
    rdfs:label "oeuvres-langues".
:s_004 a rr:SubjectMap.
:map_oeuvres-langues_000 rr:subjectMap :s_004.
:s_004 rr:template "https://mshb.huma-num.fr/prelib/oeuvre/{oeuvre_id}".
:pom_008 a rr:PredicateObjectMap.
:map_oeuvres-langues_000 rr:predicateObjectMap :pom_008.
:pm_008 a rr:PredicateMap.
:pom_008 rr:predicateMap :pm_008.
:pm_008 rr:constant crm:P72_has_language.
:pom_008 rr:objectMap :om_008.
:om_008 a rr:ObjectMap;
    rr:template "http://lexvo.org/id/iso639-3/{code_iso_639_3}";
    rr:termType rr:IRI.
